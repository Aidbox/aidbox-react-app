{
  "name": "scheduling-node-app",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "NODE_ENV=development DEBUG=telemed,@aidbox* nodemon",
    "start:production": "node build/src/index.js",
    "build": "tsc -p tsconfig.build.json && npm run copyfiles",
    "copyfiles": "copyfiles -u 1 \"src/**/*.yaml\" build",
    "lint": "eslint src/**/*",
    "test": "jest",
    "typecheck": "tsc"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aidbox/node-server-sdk": "^2.0.19",
    "aidbox-react": "^1.3.2",
    "axios": "^0.20.0",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "formdata-node": "^2.4.0",
    "shared": "0.0.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.2.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,yaml",
    "exec": "ts-node --pretty src/index.ts"
  }
