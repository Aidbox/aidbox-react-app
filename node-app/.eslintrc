{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "tsconfig.json",
    "sourceType": "module",
    "createDefaultProgram": true
  },
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": ["plugin:prettier/recommended"],
  "ignorePatterns": ["node_modules", "build", "coverage"],
  "rules": {
    "no-unused-vars": 0,
    "no-console": "off"
  }
}
