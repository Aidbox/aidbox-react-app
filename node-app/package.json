  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/js-yaml": "^4.0.2",
    "@types/luxon": "^2.0.0",
    "@types/node": "^16.4.13",
    "@types/uuid": "^8.3.1",
